//ABCD PATTERN-

#include <iostream>

using namespace std;

void abcdPattern(int n){
    
    for(int i=1; i<=n; i++){
        char alpha =65;
        
        for(int j=1; j<=n-i+1; j++){
            cout<<alpha<<" ";
            alpha=alpha+1;
        }cout<<endl;

    }
}
int main()
{
    int n; 
    cin>>n;
    abcdPattern(n);
    
    return 0;
}
